{
  "name": "marked-katex-extension",
  "version": "0.0.0",
  "description": "marked Katex extension",
  "main": "lib/marked-katex-extension.js",
  "module": "lib/marked-katex-extension.esm.js",
  "files": [
    "lib/"
  ],
  "scripts": {
    "build": "rollup -c",
    "clean": "rm -rf lib",
    "lint": "eslint .",
    "check-type": "tsc --noEmit",
