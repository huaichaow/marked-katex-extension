    "@commitlint/config-conventional": "^17.4.2",
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "^29.4.0",
    "@types/katex": "^0.16.0",
    "@types/marked": "^4.3.0",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "eslint": "^8.33.0",
    "husky": "^8.0.3",
    "rollup": "^3.14.0",
    "rollup-plugin-ts": "^3.2.0",
    "semantic-release": "^20.1.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
